{
  "name": "@booking/client",
  "version": "0.6.4",
  "author": "Emanuel Kluge (https://emanuel-kluge.de/)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && NODE_ENV=production vite build",
    "prelint": "tsc --noEmit",
    "lint": "eslint --no-ignore src",
    "test:watch": "vitest watch",
    "test": "CI=true vitest"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "jsx-a11y",
      "react-hooks"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "src/**/__tests__/*.ts?(x)"
        ],
        "plugins": [
          "testing-library"
        ]
      }
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@fluentui/merge-styles": "8.5.6",
    "@fluentui/react": "8.105.3",
    "@fluentui/theme": "2.6.22",
    "@fluentui/utilities": "8.13.6",
    "@fullcalendar/core": "6.1.1",
    "@fullcalendar/daygrid": "6.1.1",
    "@fullcalendar/interaction": "6.1.1",
    "@fullcalendar/react": "6.1.2",
    "@fullcalendar/timegrid": "6.1.1",
    "@uifabric/icons": "7.9.5",
    "classnames": "2.3.2",
    "date-fns": "2.29.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "3.1.4",
    "react-router-dom": "5.3.4"
  },
  "devDependencies": {
    "@booking/types": "0.6.3",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.4.3",
    "@types/node": "16.18.12",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "@vitejs/plugin-react-swc": "3.1.0",
    "cross-fetch": "3.1.5",
    "eslint": "8.33.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "5.10.0",
    "jsdom": "20.0.3",
    "nock": "13.3.0",
    "typescript": "4.9.5",
    "vite": "4.1.1",
    "vitest": "0.28.4"
  }
}
