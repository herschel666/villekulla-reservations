{
  "name": "@booking/client",
  "version": "0.6.6",
  "author": "Emanuel Kluge (https://emanuel-kluge.de/)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && NODE_ENV=production vite build",
    "prelint": "tsc --noEmit",
    "lint": "eslint --no-ignore src",
    "test:watch": "vitest watch",
    "test": "CI=true vitest"
  },
  "eslintConfig": {
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "plugin:prettier/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "react",
      "jsx-a11y",
      "react-hooks"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "parserOptions": {
      "sourceType": "module"
    },
    "settings": {
      "react": {
        "version": "detect"
      }
    },
    "overrides": [
      {
        "files": [
          "src/**/__tests__/*.ts?(x)"
        ],
        "plugins": [
          "testing-library"
        ]
      }
    ],
    "rules": {
      "@typescript-eslint/no-unused-vars": [
        "error",
        {
          "argsIgnorePattern": "^_",
          "varsIgnorePattern": "^_",
          "caughtErrorsIgnorePattern": "^_"
        }
      ]
    }
  },
  "dependencies": {
    "@fluentui/merge-styles": "8.5.15",
    "@fluentui/react": "8.115.6",
    "@fluentui/theme": "2.6.41",
    "@fluentui/utilities": "8.13.24",
    "@fullcalendar/core": "6.1.10",
    "@fullcalendar/daygrid": "6.1.10",
    "@fullcalendar/interaction": "6.1.10",
    "@fullcalendar/react": "6.1.10",
    "@fullcalendar/timegrid": "6.1.10",
    "@uifabric/icons": "7.9.5",
    "classnames": "2.3.2",
    "date-fns": "3.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-error-boundary": "4.0.12",
    "react-router-dom": "5.3.4"
  },
  "devDependencies": {
    "@booking/types": "0.6.3",
    "@testing-library/jest-dom": "6.4.2",
    "@testing-library/react": "14.0.0",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "18.15.3",
    "@types/react": "18.2.28",
    "@types/react-dom": "18.2.13",
    "@types/react-router-dom": "5.3.3",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react-swc": "3.4.0",
    "cross-fetch": "4.0.0",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "jsdom": "17.0.0",
    "nock": "13.3.4",
    "typescript": "5.3.3",
    "vite": "5.1.3",
    "vitest": "1.3.0"
  }
}
